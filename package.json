{
	"name": "nodejs-configurations",
	"version": "1.0.0",
	"scripts": {
		"lint:check": "eslint **/*.{js,jsx,ts,tsx}",
		"lint:fix": "eslint **/*.{js,jsx,ts,tsx} --fix",
		"format:check": "prettier . --check",
		"format:fix": "prettier . --write",
		"database:init": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
		"database:update": "sequelize db:migrate && sequelize db:seed:all",
		"compile": "tsc",
		"nodemon": "nodemon",
		"test": "webpack configtest && prettier . --check && eslint **/*.{js,jsx,ts,tsx}",
		"dev": "webpack --mode development --watch",
		"build": "webpack --mode production",
		"serve": "webpack serve --mode development"
	},
	"dependencies": {
		"mysql2": "^3.11.3",
		"sequelize": "^6.37.3"
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@babel/preset-env": "^7.25.4",
		"@babel/preset-react": "^7.24.7",
		"@types/node": "^22.5.5",
		"babel-loader": "^9.2.1",
		"css-loader": "^7.1.2",
		"eslint": "^8.57.1",
		"eslint-plugin-react": "^7.36.1",
		"eslint-plugin-spellcheck": "^0.0.20",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.6.0",
		"nodemon": "^3.1.7",
		"prettier": "^3.3.3",
		"sequelize-cli": "^6.6.2",
		"style-loader": "^4.0.0",
		"ts-loader": "^9.5.1",
		"typescript": "^5.6.2",
		"webpack": "^5.94.0",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^5.1.0"
	}
}
